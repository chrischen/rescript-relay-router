{
  "name": "r3-monorepo",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -v --topological-dev run build",
    "preview": "yarn workspaces foreach -v -pi --include 'example-*' run preview",
    "start": "yarn workspaces foreach -v -pi --include 'example-*' run start",
    "dev": "yarn workspaces foreach -v -pi --include 'example-*' run dev",
    "test": "yarn workspaces foreach -v run test",
    "test:watch": "yarn workspaces foreach -v -pi run test:watch",
    "publish": "yarn workspaces foreach -v --include 'rescript-relay-router*' run publish"
  }
}
