{
  "name": "rescript-relay-router",
  "version": "0.0.17",
  "engines": {
    "node": ">=16"
  },
  "exports": {
    "./src/*": "./src/*"
  },
  "type": "module",
  "license": "MIT",
  "author": "Gabriel Nordeborn",
  "repository": "https://github.com/zth/rescript-relay-router",
