{
  "name": "rescript-relay-router-cli",
  "version": "0.0.15-editor-tooling.1",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "main": "src/RescriptRelayRouterCli__Commands.mjs",
  "license": "MIT",
  "author": "Gabriel Nordeborn",
  "repository": "https://github.com/zth/rescript-relay-router",
  "description": "The CLI for ReScript Relay Router.",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "relay",
    "relaymodern",
    "react",
    "router",
    "react-router",
    "rescript"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rescript build -with-deps && yarn build:executable",
    "build:executable": "echo '#!/usr/bin/env node' | cat -  src/RescriptRelayRouterCli.mjs > src/RescriptRelayRouterCli.mjs.tmp && mv src/RescriptRelayRouterCli.mjs.tmp src/RescriptRelayRouterCli.mjs",
    "dev": "yarn build:rescript -w",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "publish": "yarn build && yarn npm publish"
  },
  "bin": {
    "rescript-relay-router": "src/RescriptRelayRouterCli.mjs"
  },
  "peerDependencies": {
    "rescript-relay-router": "workspace:^"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "cosmiconfig": "^7.0.1",
    "fast-fuzzy": "^1.11.1",
    "fast-glob": "^3.2.11",
    "jsonc-parser": "^3.0.0",
    "lines-and-columns": "^2.0.3",
    "magic-string": "^0.26.2",
    "vscode-jsonrpc": "^5.0.1"
  },
  "devDependencies": {
    "rescript": "9.1.4",
    "rescript-relay-router": "workspace:^",
    "rescript-relay-router-vitest": "workspace:^",
    "vitest": "0.6.0"
  }
}
