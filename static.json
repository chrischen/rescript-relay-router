{
  "root": "dist/",
  "routes": {
    "/**": "index.html"
  },
  "logging": {
    "access": true,
    "error": "error"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Cache-Control": "no-store, no-cache",
      "Access-Control-Allow-Origin": "*"
    },
    "/favicon.*": {
      "Cache-Control": "public, max-age=86400",
      "Access-Control-Allow-Origin": "*"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable",
      "Access-Control-Allow-Origin": "*"
    },
    "/img/**": {
      "Cache-Control": "public, max-age=86400",
      "Access-Control-Allow-Origin": "*"
    }
  }
}
